      <%= link_to t("layouts.login.title"), new_user_session_path,
        "data-turbolinks":"false", class: "btn btn-sm pmd-ripple-effect btn-primary" %>
    <% end %>
    <a class="btn pmd-btn-fab pmd-ripple-effect btn-link btn-sm btn-link" href="javascript:void(0);"><i class="material-icons pmd-xs">close</i></a>
  </div>
