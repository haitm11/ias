  <div class="navbar-header">
    <!--propeller logo-->
    <a target="_blank" href="#" class="navbar-brand">
    </a>
  </div>
  <!--navbar-header -->
  <div class="pmd-navbar-right-icon pull-right">	
    <% if user_signed_in? %>
      <%= link_to t("layouts.logout.title"), destroy_user_session_path,
        "data-turbolinks":"false", method: :delete, class: "btn btn-sm pmd-ripple-effect btn-primary" %>
